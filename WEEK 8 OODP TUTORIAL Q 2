#include <iostream>
using namespace std;

template <typename T>
void findMax(T a, T b) {
    if (a > b)
        cout << a << endl;
    else
        cout << b << endl;
}

int main() {
    int int1 = 10, int2 = 20;
    float float1 = 5.5f, float2 = 3.3f;
    double double1 = 12.345, double2 = 67.89;

    findMax(int1, int2);
    findMax(float1, float2);
    findMax(double1, double2);

    return 0;
}
